    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "lint-staged": "15.2.4",
    "nodemon": "3.1.0",
    "pino-pretty": "11.1.0",
    "prettier": "3.2.5",
    "prettier-plugin-organize-imports": "3.2.4",
    "typescript": "5.4.5"
  },
