{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["plugin:prettier/recommended", "eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "rules": {
    "require-atomic-updates": 0
  },
  "settings": {
    "import/resolver": "babel-root-import"
  }
}
